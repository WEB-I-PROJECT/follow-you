

<div id="sentiment-origin-template" class="form-control" type="text/x-handlebars-template">
    <div class="row">
        {{#each ValueSentimentOrigin}}
        <div class="col-md-4">
            <div class="form-control mt-3">
                <h2 class="form-control bg-group-sentiment-news">{{capitalize @key}}</h2>
                <div class="col-12">
                    {{#each this}}
                        <p class="text-kay-name form-control"><strong>{{ capitalize @key}}</strong></p>
                      <div class="text-justify form-control">
                        {{#each this}}
                         
                            <p  >{{ sentimentTag @key }} <strong>{{this}} </strong> </p>
                          
                        {{/each}}
                         </div>
                        <hr>
                    {{/each}}
                    
                </div>
                
            </div>
        </div>
        {{/each}}
    </div>
</div>
