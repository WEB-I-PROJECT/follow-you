<div id="news-backend" class="row fs-3">
    {{#each news}}
       <div class="col-12 my-3">
               <div class="new-content my-2">
                <div class="d-flex news w-100 gap-2">
                    <div class="img">
                        <img class="rounded-3" style="width: 230px;" src="{{img}}" alt="">
                    </div>
                    <div class="d-flex flex-column justify-content-between content">
                        <div class="title"><h6><b>CNN: </b>{{title}}</h6></div>
                        <div class="link"><a href="{{url}}" target="_blank"><button class="btn btn-secondary" style="font-size: 10px;">ver mais...</button></a></div>   
                    </div>
                </div>
            </div> 
    {{/each}}
</div>