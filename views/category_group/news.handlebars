<div id="tokens" class="row fs-3 overflow-auto" style="max-height: 400px;">
    {{#each news}}
        {{#each CNN}}
            <div class="new-content my-2">
                <div class="d-flex news w-100 gap-2">
                    <div class="img">
                        <img class="rounded-3" style="width: 230px; height: 130px;" src="{{this.img_src}}" alt="">
                    </div>
                    <div class="d-flex flex-column justify-content-between content">
                        <div class="title"><h6><b>CNN: </b>{{this.title}}</h6></div>
                        <div class="d-flex justify-content-between align-items-end">
                            <div class="link"><a href="{{this.link}}" target="_blank"><button class="btn btn-secondary" style="font-size: 11px;">ver mais...</button></a></div>   
                            <div class="date fs-6 text-black-50"><span>{{this.date}}</span></div>
                        </div> 
                    </div>
                </div>
            </div> 
        {{/each}}
        {{#each Folha}}
            <div class="new-content my-2">
                <div class="d-flex news w-100 gap-2">
                    <div class="img">
                        <img class="rounded-3" style="width: 230px; height: 130px;" src="{{this.img_src}}" alt="">
                    </div>
                    <div class="d-flex flex-column justify-content-between content">
                        <div class="title"><h6><b>FOLHA: </b>{{this.title}}</h6></div>
                        <div class="d-flex justify-content-between align-items-end">
                            <div class="link"><a href="{{this.link}}" target="_blank"><button class="btn btn-secondary" style="font-size: 10px;">ver mais...</button></a></div>   
                            <div class="date fs-6 text-black-50"><span>{{this.date}}</span></div>
                        </div>
                    </div>
                </div>
            </div> 
        {{/each}}
        {{#each IG}}
            <div class="new-content my-2">
                <div class="d-flex news w-100 gap-2">
                    <div class="img">
                        <img class="rounded-3" style="width: 230px; height: 130px;" src="{{this.img_src}}" alt="">
                    </div>
                    <div class="d-flex flex-column justify-content-between content">
                        <div class="title"><h6><b>IG: </b>{{this.title}}</h6></div>
                        <div class="d-flex justify-content-between align-items-end">
                            <div class="link"><a href="{{this.link}}" target="_blank"><button class="btn btn-secondary" style="font-size: 10px;">ver mais...</button></a></div>   
                            <div class="date fs-6 text-black-50"><span>{{this.date}}</span></div>
                        </div>

                    </div>
                </div>
            </div> 
        {{/each}}
    {{/each}}
</div>