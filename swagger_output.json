  "definitions": {
    "WordsFrequency": {
      "type": "object",
      "properties": {
        "mostFrequentWordsInContent": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "word": {
                "type": "string",
                "example": "ensino"
              },
              "frequency": {
                "type": "number",
                "example": 732
              }
            }
          }
        }
      }
    },
    "WordsCharts": {
      "type": "object",
      "properties": {
        "labels": {
          "type": "array",
          "example": [
            "3-3-2021",
            "9-11-2021",
            "7-2-2022"
          ],
          "items": {
            "type": "string"
          }
        },
        "data": {
          "type": "array",
          "example": [
            1,
            1,
            1
          ],
          "items": {
            "type": "number"
          }
        }
      }
    },
    "NewsKeyword": {
      "type": "object",
      "properties": {
        "news": {
          "type": "array",
